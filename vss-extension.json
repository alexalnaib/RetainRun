{
    "manifestVersion": 1,
    "id": "retain-run",
    "name": "Retain Run",
    "version": "1.0.0",
    "publisher": "alexalnaib",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Retains Azure DevOps pipeline run",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "overview": {
            "path": "Overview.md"
        }
    },
    "files": [
        {
            "path": "task"
        },
        {
            "path": "images"
        }
    ],
    "contributions": [
        {
            "id": "retain-run",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}